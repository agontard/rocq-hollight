opam-version: "2.0"
synopsis: "HOL-Light library in Rocq"
description: """
This library contains an automatic translation in Rocq of the HOL-Light library
using hol2dk and lambdapi.
"""
depends: [
  "rocq-prover" {= "9.0.0"}
  "coq-mathcomp-classical" {= "1.15.0"}
  "coq-mathcomp-analysis-stdlib" {= "1.15.0"}
  "coq-fourcolor-reals" {= "1.4.2"}
  "coq-mathcomp-zify" {= "1.6.0+2.3+8.18"}
  "rocq-equations" {= "1.3.1+9.0"}
]
build: [make "-j%{jobs}%"]
install: [make "install"]
